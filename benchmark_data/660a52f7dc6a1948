      <category>Colombia</category>
      <category>Gustavo Petro</category>
      <category>Mauricio Cárdenas</category>
      <category>Abelardo de la Espriella</category>
      <category>Iván Cepeda</category>
      <category>Paloma Valencia</category>
      <media:content url="https://images.english.elpais.com/resizer/v2/6QE5XBLVDJHWBDNJF6B2CX5BR4.jpg?auth=6671e4f40c4a6787b7983d574f8b3f60e7dfacab20d0384ca8a092e2d8c3697c" type="image/jpeg" medium="image">
