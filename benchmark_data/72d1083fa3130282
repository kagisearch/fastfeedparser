      <title>New Year, New Nuance</title>
      <itunes:episode>53</itunes:episode>
      <podcast:episode>53</podcast:episode>
      <itunes:title>New Year, New Nuance</itunes:title>
      <itunes:episodeType>full</itunes:episodeType>
      <guid isPermaLink="false">0df74dff-9aee-41ab-8892-58caea5a37bd</guid>
